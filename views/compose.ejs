<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="footer-padding " action="/compose" method="post">
  <div class="form-group">
    <label for="newContent">Title</label>
    <input type="text" class="form-control" name="newTitle" value="">
  </div>
  <div class="form-group">
    <label for="newContent">Post</label>
    <textarea name="newPost" class="form-control" rows="8" cols="100"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%- include("partials/footer") -%>
